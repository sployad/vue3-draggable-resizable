{
  "name": "draggable-resizable-vue-v3",
  "version": "1.1.2",
  "private": false,
  "description": "Draggable resizable for vue3",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:dts",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name Vue3DraggableResizable src/index.ts",
    "build:dts": "tsc src/index.ts --declaration --declarationDir typings"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/compiler-sfc": "^3.3.4",
    "babel-plugin-import": "^1.13.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "typescript": "~4.9.3",
    "core-js": "^3.6.5",
    "vue": "^3.2.13"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sployad/vue3-draggable-resizable.git"
  },
  "keywords": [
    "vue3",
    "component",
    "draggable",
    "resizable"
  ],
  "main": "src/index.js",
  "module": "src/index.js",
  "unpkg": "src/index.js",
  "typings": "typings/index.d.ts"
}
