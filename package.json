{
  "name": "draggable-resizable-vue-v3",
  "version": "1.1.0",
  "private": false,
  "description": "Draggable resizable for vue3",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:dts",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name Vue3DraggableResizable src/index.ts",
    "build:dts": "tsc src/index.ts --declaration --declarationDir typings"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-plugin-import": "^1.13.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "typescript": "~3.9.3",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sployad/vue3-draggable-resizable.git"
  },
  "keywords": [
    "vue3",
    "component",
    "draggable",
    "resizable"
  ],
  "main": "src/index.js",
  "module": "src/index.js",
  "unpkg": "src/index.js",
  "typings": "typings/index.d.ts"
}
